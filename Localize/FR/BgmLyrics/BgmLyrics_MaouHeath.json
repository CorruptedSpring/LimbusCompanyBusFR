    {
      "id": 0,
      "content": "<color=#930380>Combien ai-je pu souhaiter pour un autre</color>",
      "vocalid": 0
    },
    {
      "id": 1,
      "content": "<color=#930380>futur radieux, merveilleux, meilleur</color>",
      "vocalid": 0
    },
    {
      "id": 2,
      "content": "<color=#930380>Me voilà devant le portail</color>",
      "vocalid": 0
    },
    {
      "id": 3,
      "content": "<color=#930380>Cher bon vieux temps</color>",
      "vocalid": 0
    },
    {
      "id": 4,
      "content": "<color=#930380>Je me suis décidé</color>",
      "vocalid": 0
    },
    {
      "id": 5,
      "content": "<color=#930380>Combien ma colère a dû te faire souffrir</color>",
      "vocalid": 0
    },
    {
      "id": 6,
      "content": "<color=#930380>Sang-froid vulgaire et maladroit</color>",
      "vocalid": 0
    },
    {
      "id": 7,
      "content": "Combien as-tu dû négliger tes merveilles",
      "vocalid": 1
    },
    {
      "id": 8,
      "content": "Manières directes et sincères",
      "vocalid": 1
    },
    {
      "id": 9,
      "content": "<color=#930380>Nous voici sur cette terre prédestinée</color>",
      "vocalid": 0
    },
    {
      "id": 10,
      "content": "<color=#930380>J’y mettrai un terme</color>",
      "vocalid": 0
    },
    {
      "id": 11,
      "content": "Me regarderas-tu y mettre un terme ?",
      "vocalid": 1
    },
    {
      "id": 12,
      "content": "<color=#930380>Oh</color>",
      "vocalid": 0
    },
    {
      "id": 13,
      "content": "<color=#930380>Nous tournons en rond</color>",
      "vocalid": 0
    },
    {
      "id": 14,
      "content": "<color=#930380>Nous accrochons à la douleur</color>",
      "vocalid": 0
    },
    {
      "id": 15,
      "content": "<color=#930380>Entraînée par nos egos</color>",
      "vocalid": 0
    },
    {
      "id": 16,
      "content": "<color=#930380>Sentiments indicibles</color>",
      "vocalid": 0
    },
    {
      "id": 17,
      "content": "<color=#930380>Tissant nos propres versions du passé</color>",
      "vocalid": 0
    },
    {
      "id": 18,
      "content": "<color=#930380>Que tu ne peux savoir</color>",
      "vocalid": 0
    },
    {
      "id": 19,
      "content": "<color=#930380>Car je sais</color>",
      "vocalid": 0
    },
    {
      "id": 20,
      "content": "<color=#930380>Je dois être la raison pour laquelle</color>",
      "vocalid": 0
    },
    {
      "id": 21,
      "content": "<color=#930380>Tu as abandonné tes sourires</color>",
      "vocalid": 0
    },
    {
      "id": 22,
      "content": "<color=#930380>Et que l’espoir dans tes yeux a été volé</color>",
      "vocalid": 0
    },
    {
      "id": 23,
      "content": "<color=#930380>Je dois être la raison pour laquelle</color>",
      "vocalid": 0
    },
    {
      "id": 24,
      "content": "<color=#930380>Tu dois me dire tous ces mensonges</color>",
      "vocalid": 0
    },
    {
      "id": 25,
      "content": "<color=#930380>À te souhaiter une meilleure vie sans moi à tes côtés</color>",
      "vocalid": 0
    },
    {
